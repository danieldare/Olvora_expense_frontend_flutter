<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Sign in with Apple -->
	<key>com.apple.developer.applesignin</key>
	<array>
		<string>Default</string>
	</array>

	<!-- App Groups - Enable data sharing between Olvora apps -->
	<key>com.apple.security.application-groups</key>
	<array>
		<string>group.com.olvora.shared</string>
	</array>

	<!-- Associated Domains for Universal Links -->
	<key>com.apple.developer.associated-domains</key>
	<array>
		<string>applinks:olvora.com</string>
		<string>applinks:expenses.olvora.com</string>
	</array>

	<!-- Push Notifications -->
	<key>aps-environment</key>
	<string>production</string>

	<!-- Keychain Sharing - Share credentials across Olvora apps -->
	<key>keychain-access-groups</key>
	<array>
		<string>$(AppIdentifierPrefix)com.olvora.shared</string>
	</array>
</dict>
</plist>
